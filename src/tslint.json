{
    "extends": "../tslint.json",
    "rules": {
        "directive-selector": [
            true,
            "attribute",
            "app",
            "camelCase"
        ],
        "component-selector": [
            true,
            "element",
            "app",
            "kebab-case"
        ],
        "no-string-literal": false,
        "no-trailing-whitespace": false,
        "object-literal-shorthand": false,
        "no-conditional-assignment": false
    }
}
